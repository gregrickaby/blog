{
  "name": "@gregrickaby/blog",
  "version": "1.0.0",
  "private": true,
  "description": "My blog.",
  "author": "Greg Rickaby <gregrickaby@gmail.com>",
  "license": "MIT",
  "keywords": [
    "blog"
  ],
  "homepage": "https://gregrickaby.blog",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregrickaby/blog.git"
  },
  "bugs": {
    "url": "https://github.com/gregrickaby/blog/issues"
  },
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next",
    "dev": "npm run clean && next dev",
    "export": "next export",
    "format": "npx prettier '**/*.{js,jsx,ts,tsx,md,mdx,html,css,scss,json,yml}' --config=.prettierrc.js --write || true",
    "lint": "npm run format && npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:css": "npx stylelint '**/*.css' --config=.stylelintrc.js --fix || true",
    "lint:js": "npx eslint '**/*.{js,ts,jsx,tsx}' --fix || true && next lint",
    "lint:md": "npx markdownlint '**/*.md' --config=.markdownlint.json --fix",
    "postbuild": "next-sitemap",
    "start": "next start"
  },
  "dependencies": {
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/core": "^7.17.5",
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@formium/client": "^0.1.4",
    "@formium/react": "^0.2.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "classnames": "2.3.1",
    "dayjs": "^1.10.7",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gray-matter": "^4.0.3",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.1",
    "next-mdx-remote": "3.0.8",
    "next-seo": "^5.1.0",
    "next-sitemap": "^2.4.6",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.6",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "prism-react-renderer": "^1.3.1",
    "react-google-recaptcha": "^2.1.0",
    "reading-time": "^1.5.0",
    "remark-oembed": "^1.2.2",
    "remark-prism": "^1.3.6",
    "remark-slug": "^7.0.1",
    "rimraf": "^3.0.2",
    "sharp": "^0.30.1",
    "stylelint": "^14.5.2",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "3.0.23"
  }
}
