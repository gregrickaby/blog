{
  "name": "gregrickaby-blog",
  "version": "0.1.0",
  "private": true,
  "description": "My blog built on Next.js and hosted at Vercel.",
  "author": "Greg Rickaby <gregrickaby@gmail.com>",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "next.js",
    "blog"
  ],
  "homepage": "https://github.com/gregrickaby/gregrickaby-blog/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregrickaby/gregrickaby-blog.git"
  },
  "bugs": {
    "url": "https://github.com/gregrickaby/gregrickaby-blog/issues"
  },
  "engines": {
    "node": "14",
    "npm": "7"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next",
    "deploy prod": "vercel --prod",
    "deploy staging": "vercel",
    "dev": "npm run clean && next dev",
    "develop": "npm run dev",
    "export": "next export",
    "format": "npx prettier '**/*.{js,jsx,ts,tsx,md,mdx,html,css,scss,json,yml}' --config=.prettierrc.js --write || true",
    "lint": "npm run format && npm run lint:js && npm run lint:css",
    "lint:css": "npx stylelint '**/*.css' --config=.stylelintrc.js --fix || true",
    "lint:js": "npx eslint './' --config=.eslintrc.js --fix || true",
    "lint:md": "npx markdownlint '**/*.{md,mdx}' --config=.markdownlint.json --fix",
    "postbuild": "next-sitemap",
    "start": "next start"
  },
  "dependencies": {
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@babel/core": "^7.14.3",
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@formium/client": "^0.1.3",
    "@formium/react": "^0.1.3",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "classnames": "2.3.1",
    "dayjs": "^1.10.5",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "exifr": "^7.0.0",
    "fraction.js": "^4.1.1",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.0",
    "markdownlint": "^0.23.1",
    "markdownlint-cli": "^0.27.1",
    "next-mdx-remote": "3.0.2",
    "next-seo": "^4.24.0",
    "next-sitemap": "^1.6.102",
    "nprogress": "^0.2.0",
    "postcss": "^8.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "prism-react-renderer": "^1.2.1",
    "react-google-recaptcha": "^2.1.0",
    "remark-oembed": "^1.2.2",
    "remark-prism": "^1.3.6",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "2.1.4",
    "use-dark-mode": "^2.3.1"
  }
}
